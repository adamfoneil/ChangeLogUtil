@model ChangeLog.Web.ViewModels.ObjectVersion

<h5>version @Model.EventView.Version</h5>

<div class="diffPane">
    <table cellpadding="0" cellspacing="0" class="diffTable">
        @foreach (var diffLine in Model.PaneModel.Lines)
        {
        <tr>
            <td class="lineNumber">
                @Html.Raw(diffLine.Position.HasValue? diffLine.Position.ToString() : "&nbsp;")
            </td>
            <td class="line @diffLine.Type.ToString()Line">
                <span class="lineText">
                    <partial name="_DiffLine" model="diffLine"/>                
                </span>
            </td>
        </tr>
        }
    </table>
</div>